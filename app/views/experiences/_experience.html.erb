<div id="<%= dom_id experience %>">
  <p>
    <strong>Title:</strong>
    <%= experience.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= experience.description %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= experience.company %>
  </p>

  <p>
    <strong>Date begin:</strong>
    <%= experience.date_begin %>
  </p>

  <p>
    <strong>Date finish:</strong>
    <%= experience.date_finish %>
  </p>

  <p>
    <strong>Tags:</strong>
    <%= experience.tags %>
  </p>

</div>
